{
    "name": "Fmovies Watch History",
    "description": "Store users watch history and display episodes that have already been watched",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://fmovies.to/*"],
            "js": [
                "content-script.js"
            ],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "tabs", "activeTab"
    ]
}